#: ../admin/view/autolinks.php:409 ../admin/view/autolinks.php:424
#: ../admin/view/autolinks.php:547 ../admin/view/autolinks.php:562
msgid "White Space"
msgstr "Spazio Bianco"

#: ../admin/class-daim-admin.php:1899 ../admin/class-daim-admin.php:1932
#: ../admin/view/autolinks.php:410 ../admin/view/autolinks.php:425
#: ../admin/view/autolinks.php:548 ../admin/view/autolinks.php:563
